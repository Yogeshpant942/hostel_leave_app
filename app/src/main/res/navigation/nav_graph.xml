        <action
            android:id="@+id/action_studentMainFragment_to_leaveHistoryFragment"
            app:destination="@id/leaveHistoryFragment" />
        <action
            android:id="@+id/action_studentMainFragment_to_leaveApplyFragment"
            app:destination="@id/leaveApplyFragment" />
        <action
            android:id="@+id/action_studentMainFragment_to_roleSelectFragment"
