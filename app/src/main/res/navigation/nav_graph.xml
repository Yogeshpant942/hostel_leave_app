    <fragment
        android:id="@+id/allOutStudentFragment"
        android:name="com.example.hostelleaveapp.Ui.Guard.AllOutStudentFragment"
        android:label="AllOutStudentFragment" />
    <fragment
        android:id="@+id/guardSignUpFragment"
        android:name="com.example.hostelleaveapp.Ui.Guard.GuardSignUpFragment"
        android:label="GuardSignUpFragment" >
        <action
            android:id="@+id/action_guardSignUpFragment_to_guardMainScreenFragment"
            app:destination="@id/guardMainScreenFragment" />
