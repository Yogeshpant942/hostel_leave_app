    <fragment
        android:id="@+id/guardLoginFragment"
        android:name="com.example.hostelleaveapp.Ui.Guard.GuardLoginFragment"
        android:label="GuardLoginFragment" >
        <action
            android:id="@+id/action_guardLoginFragment_to_guardSignUpFragment"
            app:destination="@id/guardSignUpFragment" />
        <action
            android:id="@+id/action_guardLoginFragment_to_guardMainScreenFragment"
            app:destination="@id/guardMainScreenFragment" />
    </fragment>
